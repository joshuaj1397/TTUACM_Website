{
  "extends": "airbnb-base",
  "rules": {
    "linebreak-style": "off",
    "no-use-before-define": "off",
    "no-console": "off",
    "prefer-destructuring": "off",
    "no-undef": "off",
    "no-underscore-dangle": "off",
    "no-shadow": "off",
    "prefer-arrow-callback": "off",
    "comma-dangle": "off",
    "object-curly-spacing": "off",
    "no-multi-assign": "off",
    "function-paren-newline": "off",
    "arrow-body-style": "off",
    "no-unused-vars": "off",
    "no-mixed-operators": "off",
    "global-require": "off"
  }
}
